{
	"ConnectionStrings": {
		"DefaultConnection": "Server=127.0.0.1;Port=5432;Database=Hospital;Username=postgres;Password=pass"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"NLog": {
		"rules": [
			{
				"logger": "*",
				"minLevel": "Info",
				"writeTo": "logconsole"
			},
			{
				"logger": "*",
				"minLevel": "Debug",
				"writeTo": "logfile"
			}
		]
	},
	"ApplicationContextStartupOptions": {
		"InitializeUserLogin": "admin",
		"InitializeUserPassword": "admin"
	}
}